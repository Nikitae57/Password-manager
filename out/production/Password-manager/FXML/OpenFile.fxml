<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="VBox_openFile" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="54.0" minWidth="194.0" prefHeight="144.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.OpenFileController">
   <children>
      <HBox fx:id="HBox_open_new" prefHeight="40.0" prefWidth="181.0">
         <children>
            <Button fx:id="btn_open" minHeight="30.0" minWidth="70.0" mnemonicParsing="false" onAction="#openFile" text="Открыть">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btn_newFile" cancelButton="true" minHeight="30.0" minWidth="70.0" mnemonicParsing="false" onAction="#createNewFile" prefWidth="60.0" text="Новый">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <Label text="Пароль:">
         <VBox.margin>
            <Insets left="20.0" top="10.0" />
         </VBox.margin>
      </Label>
      <HBox fx:id="HBox_password" prefHeight="30.0" prefWidth="200.0">
         <children>
            <PasswordField fx:id="passwordField_password" disable="true" prefHeight="25.0" prefWidth="159.0" promptText="Введите пароль">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
            </PasswordField>
         </children>
      </HBox>
      <HBox prefHeight="61.0" prefWidth="200.0">
         <children>
            <Button cancelButton="true" minHeight="30.0" minWidth="70.0" mnemonicParsing="false" onAction="#exit" text="Выйти">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="btn_Apply" defaultButton="true" disable="true" minHeight="30.0" minWidth="70.0" mnemonicParsing="false" onAction="#apply" text="Принять">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
